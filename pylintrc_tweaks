# pylintrc tweaks for use with edx_lint.
[MASTER]
ignore = migrations
load-plugins = edx_lint.pylint,pylint_django,pylint_celery

[MESSAGES CONTROL]
disable =
    duplicate-code,
    invalid-name,
    too-few-public-methods,
    too-many-public-methods,
    unspecified-encoding,
    use-maxsplit-arg,
